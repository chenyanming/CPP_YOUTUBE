#include <iostream>

using namespace std;

int main()
{
  int line[5] = {1,2,3,4,5};
  int line1[5];
  line1[0] = 5;
  line1[1] = 3;
 // line[2] = 9;
  line1[3] = 7;
  line1[4] = 8;
  cout << "Person 2 has " << line[2] << "$" << endl;
  cout << "Person 8 has " << line[8] << "$" << endl;
  cout << "Person 2 has " << line1[2] << "$" << endl;
  return 0;
}